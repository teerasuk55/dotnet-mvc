@{
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
    ViewData["Title"] = "Endform";  
}
    
    &nbsp&nbsp
    <div class="card ">
            <div class="card-header">
                <h3 class="card-title">แบบฟอร์มคำขออนุมัติไปปฏิบัติงานโดยไม่ขอเบิกค่าใช้จ่าย</h3>
            </div>
            <form>
                <div class="card-body">
                    <div class="form-group">
                        <label for="Number">หมายเลขเอกสารที่</label>
                        <input type="text" class="form-control" id="Number" placeholder="หมายเลขเอกสารที่" disabled>
                    </div>
                    <div class="form-group">
                        <label for="Name">รายชื่อ</label>
                        <div id="inputFormRow">
                            <div class="input-group mb-2">
                                <input type="text" name="Name[]" class="form-control m-input" placeholder="ชื่อ - สกุล" autocomplete="off" disabled>
                                <input type="text" name="Rank[]" class="form-control m-input" placeholder="ตำแหน่งทางวิชาการ" autocomplete="off" disabled>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Where">เดินทางไปปฏิบัติงานที่</label>
                        <input type="text" class="form-control" id="Where" placeholder="เดินทางไปปฏิบัติงานที่" disabled>
                    </div>
                    <div class="form-group">
                        <label for="Province">จังหวัด</label>
                        <select class="form-control" id="District" disabled>
                            <option selected>เลือกจังหวัด...</option>
                            <option value="1">หนึ่ง</option>
                            <option value="2">สอง</option>
                            <option value="3">สาม</option>
                            <option value="4">สี่</option>
                            <option value="5">ห้า</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="District">อำเภอ</label>
                        <select class="form-control" id="District" disabled>
                            <option selected>เลือกอำเภอ...</option>
                            <option value="1">หนึ่ง</option>
                            <option value="2">สอง</option>
                            <option value="3">สาม</option>
                            <option value="4">สี่</option>
                            <option value="5">ห้า</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="Tumbon">ตำบล</label>
                        <select class="form-control" id="Tumbon" disabled>
                            <option selected>เลือกตำบล...</option>
                            <option value="1">หนึ่ง</option>
                            <option value="2">สอง</option>
                            <option value="3">สาม</option>
                            <option value="4">สี่</option>
                            <option value="5">ห้า</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label>ตั้งแต่วันที่(เดือน/วัน/ปี)</label>
                        <div class="input-group date" id="reservationdate" data-target-input="nearest">
                            <input type="text" class="form-control datetimepicker-input" data-target="#reservationdate" placeholder="เดือน/วัน/ปี" disabled/>
                            <div class="input-group-append" data-target="#reservationdate" data-toggle="datetimepicker">
                                <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label>ถึงวันที่(เดือน/วัน/ปี)</label>
                        <div class="input-group date" id="reservationdate1" data-target-input="nearest">
                            <input type="text" class="form-control datetimepicker-input" data-target="#reservationdate1" placeholder="เดือน/วัน/ปี" disabled/>
                            <div class="input-group-append" data-target="#reservationdate1" data-toggle="datetimepicker">
                                <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Total">รวมเป็นเวลา</label>
                        <div class="input-group">
                            <input type="text" name="Total" class="form-control" id="Total" placeholder="รวมเป็นเวลา" disabled>
                        <div class="input-group-append">
                            <span class="input-group-text">วัน</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Reason">เหตุผล</label>
                        <input type="text" class="form-control" id="Reason" placeholder="เหตุผล" disabled>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputFile">เอกสารเพิ่มเติม</label>
                        <div class="input-group">
                            <div class="custom-file">
                                <input type="file" class="custom-file-input" id="exampleInputFile" disabled>
                                <label class="custom-file-label" for="exampleInputFile">Choose file</label>
                            </div>
                            <div class="input-group-append">
                                <button class="btn btn-outline-secondary" type="button"><i class="fa fa-download"></i>Download</button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <div class="card-footer">
                <a type="submit" class="btn btn-warning text-white"><i class="fas fa-window-close"></i>  ยกเลิกแบบฟอร์ม</a>
            </div>
        </div>

@section Styles
{
    <!-- daterange picker -->
    <link rel="stylesheet" href="~/adminlte/plugins/daterangepicker/daterangepicker.css">
    <!-- iCheck for checkboxes and radio inputs -->
    <link rel="stylesheet" href="~/adminlte/plugins/icheck-bootstrap/icheck-bootstrap.css">
    <!-- Bootstrap Color Picker -->
    <link rel="stylesheet" href="~/adminlte/plugins/bootstrap-colorpicker/css/bootstrap-colorpicker.min.css">
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet" href="~/adminlte/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- Select2 -->
    <link rel="stylesheet" href="~/adminlte/plugins/select2/css/select2.min.css">
    <link rel="stylesheet" href="~/adminlte/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css">
    <!-- Bootstrap4 Duallistbox -->
    <link rel="stylesheet" href="~/adminlte/plugins/bootstrap4-duallistbox/bootstrap-duallistbox.min.css">
    <!-- BS Stepper -->
    <link rel="stylesheet" href="~/adminlte/plugins/bs-stepper/css/bs-stepper.min.css">
    <!-- dropzonejs -->
    <link rel="stylesheet" href="~/adminlte/plugins/dropzone/min/dropzone.min.css">
}

@section Scripts
{
    <!-- bs-custom-file-input -->
    <script src="~/adminlte/plugins/bs-custom-file-input/bs-custom-file-input.min.js"></script>
    <!-- Select2 -->
    <script src="~/adminlte/plugins/select2/js/select2.full.min.js"></script>
    <!-- Bootstrap4 Duallistbox -->
    <script src="~/adminlte/plugins/bootstrap4-duallistbox/jquery.bootstrap-duallistbox.min.js"></script>
    <!-- InputMask -->
    <script src="~/adminlte/plugins/moment/moment.min.js"></script>
    <script src="~/adminlte/plugins/inputmask/jquery.inputmask.min.js"></script>
    <!-- date-range-picker -->
    <script src="~/adminlte/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- bootstrap color picker -->
    <script src="~/adminlte/plugins/bootstrap-colorpicker/js/bootstrap-colorpicker.min.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/adminlte/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Bootstrap Switch -->
    <script src="~/adminlte/plugins/bootstrap-switch/js/bootstrap-switch.min.js"></script>
    <!-- BS-Stepper -->
    <script src="~/adminlte/plugins/bs-stepper/js/bs-stepper.min.js"></script>
    <!-- dropzonejs -->
    <script src="~/adminlte/plugins/dropzone/min/dropzone.min.js"></script>
    <script>
        $(function () {
            bsCustomFileInput.init();
        });
        $(function () {
            //Date picker
            $('#reservationdate').datetimepicker({
                format: 'L'
            });
            //Date picker 1
            $('#reservationdate1').datetimepicker({
                format: 'L'
            });
        });
    </script>
}